[core]
  repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
[remote "website"]
	url = https://github.com/hhua1/hw2.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
  remote = website
  merge = refs/heads/master